{
  "schemaVersion": 1,
  "id": "focalworks",
  "version": "${version}",
  "name": "Focalworks",
  "description": "Focalworks addon for Hex Casting",
  "authors": [
    "c-Caelum"
  ],
  "contact": {
    "homepage": "https://c-caelum.github.io/Focalworks",
    "sources": "https://github.com/c-Caelum/Focalworks"
  },
  "license": "MIT",
  "icon": "assets/focalworks/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [{
      "adapter": "kotlin",
      "value": "io.github.c_caelum.focalworks.fabric.FabricFocalworks"
    }],
    "client": [{
      "adapter": "kotlin",
      "value": "io.github.c_caelum.focalworks.fabric.FabricFocalworksClient"
    }],
    "server": [{
      "adapter": "kotlin",
      "value": "io.github.c_caelum.focalworks.fabric.FabricFocalworksServer"
    }],
    "fabric-datagen": [{
      "adapter": "kotlin",
      "value": "io.github.c_caelum.focalworks.fabric.datagen.FabricFocalworksDatagen"
    }],
    "modmenu": [{
      "adapter": "kotlin",
      "value": "io.github.c_caelum.focalworks.fabric.FabricFocalworksModMenu"
    }]
  },
  "mixins": [
    "focalworks-common.mixins.json",
    "focalworks-fabric.mixins.json"
  ],
  "depends": {
    "minecraft": "${versions.minecraft}",
    "fabricloader": ">=${versions.fabric_loader}",
    "fabric-api": ">=${versions.fabric_api}",
    "fabric-language-kotlin": ">=${versions.kotlin_fabric}",
    "architectury": ">=${versions.architectury}",
    "hexcasting": ">=${versions.hexcasting}",
    "cloth-config": ">=${versions.clothConfig}"
  },
  "suggests": {
    "modmenu": ">=${versions.modMenu}"
  }
}
